{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["react-app", "prettier","plugin:react/recommended",
        "airbnb"],
    "plugins": ["react", "prettier",  "@typescript-eslint","@typescript-eslint/parser"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "trailingComma": "es5",
          "semi": false,
          "jsxSingleQuote": true,
          "singleQuote": true,
          "useTabs": true
        }
      ]
    }
  }